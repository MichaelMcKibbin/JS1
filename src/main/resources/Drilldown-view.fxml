<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.michaelmckibbin.js1.DrilldownController">


    <SplitPane dividerPositions="0.19539078156312625" focusTraversable="true" VBox.vgrow="ALWAYS">
        <AnchorPane>
            <VBox alignment="TOP_CENTER" layoutX="27.0" layoutY="20.0" prefHeight="122.0" prefWidth="100.0"
                  spacing="10" styleClass="navigation-button-container-1" AnchorPane.topAnchor="20.0">
                <Button fx:id="StorefrontButton" layoutX="10.0" layoutY="2.0" mnemonicParsing="false"
                        onAction="#handleStorefrontButtonClick" styleClass="storefront-button" text="Storefront"/>
                <Button fx:id="displayCasesButton" layoutX="10.0" layoutY="12.0" mnemonicParsing="false"
                        onAction="#handleDisplayCasesButtonClick" styleClass="storefront-button"
                        text="Display Cases"/>
                <Button fx:id="drilldownRefreshViewButton" mnemonicParsing="false" onAction="#drilldownRefresh"
                        text="Refresh View" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold Italic" size="14.0"/>
                    </font>
                </Button>
                <padding>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </padding>
            </VBox>
        </AnchorPane>


        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <ScrollPane>
                <TreeView fx:id="inventoryTreeView" prefHeight="750.0" prefWidth="760.0" VBox.vgrow="ALWAYS"/>
            </ScrollPane>
        </AnchorPane>


    </SplitPane>

</VBox>
